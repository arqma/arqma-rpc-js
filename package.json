    "test-wallet-multisig-transfer": "mocha -t 60000 ./test/wallet/walletMultisigTransfer.js",
    "test-wallet-proofs": "mocha -t 60000 ./test/wallet/walletProofs.js",
    "test-wallet-relay": "mocha -t 60000 ./test/wallet/walletRelay.js",
